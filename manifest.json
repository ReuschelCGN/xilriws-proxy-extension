{
  "manifest_version": 3,
  "name": "Proxy",
  "version": "28.0.0",
  "options_page": "options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "options.html"
  },
  "permissions": [
    "tabs",
    "proxy",
    "unlimitedStorage",
    "storage",
    "webRequest",
    "webRequestBlocking",
    "webNavigation",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "minimum_chrome_version":"22.0.0"
}
